{
  "source": {
    "include": ["../../Daemon/ctrlKafka",
                "../../Daemon/DHDaemon",
                "../../Lib", "../../RMSync", "../../SessionManager", "../../VersionControl"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["node_modules/jsdoc/plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "../../docs/"
  }
}
